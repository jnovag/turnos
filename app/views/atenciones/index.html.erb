

<h1> Atenciones</h1>

<table class="table table-striped table-hover table-condensed">
  <thead>
    <tr>
      <th>Tiempo Atenci√≥n</th>
      <th>Nombre</th>
      <th>Descripcion</th>
      <th>Comentario</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @atenciones.each do |atencion| %>
      <tr>
        <td><%= distance_of_time_in_words_to_now(atencion.hora_atencion) %></td>
        <td><%= atencion.nombre %></td>
        <td><%= atencion.descripcion %></td>
        <td><%= atencion.comentario %></td>
        <td><%= link_to 'Show', atencion %></td>
        <td><%= link_to 'Edit', edit_atencion_path(atencion) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

